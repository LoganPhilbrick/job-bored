
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id            String   @id @default(cuid())
  url           String   @unique
  title         String
  companyName   String
  jobType       String
  postedAt      DateTime
  location      String
  salary        String
  description   String
}